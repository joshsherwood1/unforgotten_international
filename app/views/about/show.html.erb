    <script>
      window._wq = window._wq || [];
      _wq.push({ id: "_all", onReady: function(video) {
        video.bind("pause", function() {

          analytics.track('Pause', {
            category: 'Video',
            label: `${video.name()}`
          })
        });

        video.bind("play", function() {

          analytics.track({
          event: "Play",
          properties: {
            category: 'Video',
            label: `${video.name()}`
          }
        });

        video.bind("end", function() {

          analytics.track('End', {
            category: 'Video',
            label: `${video.name()}`
          })
        });
      }});
    </script>
