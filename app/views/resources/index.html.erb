<h2 style="text-align: center">Resources</h2>
<br>
<br>
<% @resources.each do |resource| %>
    <a href="<%= resource.website_url %>" target="_blank"><h1 style="text-align: center"><%= resource.name %></h1></a>
    <p style="text-align: center"><%= resource.summary %></p>
    <br>
    <div class = "center">
      <a href="https://twitter.com/share?ref_src=twsrc%5Etfw" class="twitter-share-button" data-size="large" data-text="<%= resource.name %>" data-lang="en" data-show-count="false">Tweet</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
    <br>
    </div>
  <% resource.medias.each do |media| %>
  <div class = "center">
    <%= media.embedded_html.html_safe %>
  </div>
    <br>
  <% end %>
<% end %>
